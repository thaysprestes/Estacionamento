@model List<Carro>


<div class="row">
    <div class="col-md-4">
        <div class="bitcoin">
            <h2>Bitcoin R$ - Compra</h2>
            <h3 class="realCompra"></h3>
        </div>
     </div>
</div>
<br />
<hr />


<div class="row">
    @foreach (Carro carroCadastrado in Model)
    {
    <div class="col-sm-3 div-conteudo">
        <img src="~/images/icon.ico" alt="Imagem de Carro" style="width:100px;height:80px"/>
        <h1>@carroCadastrado.Placa</h1>
        <h2>@carroCadastrado.Modelo</h2>
    </div>
    }
</div>





<script src="~/js/site.js"></script>

